<nav class="navbar navbar-expand-lg navbar-light" style="background-color: #94c481;">
    <div class="row" style="width: 100%">
        <div class="col">
            <div class="collapse navbar-collapse" id="navbarNav" style="padding: 12px 0px">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link" href="/home">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/generate">Generate</a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/profile" onclick="return false;">Profile<span class="sr-only">(current)</span></a>
                    </li>
                </ul>
            </div>
        </div>
        <div class="col-4" style="display: inherit;">
            <img class="profImg" src={{user.profilePicUrl}}>
            <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                {{user.username}}
            </button>
            <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                <a class="dropdown-item" href="/logout">Logout</a>
            </div>
        </div>
    </div>
</nav>
<main style="background-color: #f1c344;">
    <div class="container" style="background-color: #d9ac32;">
        {{#if patterns}}
            {{#each patterns}}
                <div class="card" style="width: 18rem;">
                <img class="card-img-top imgForCard" id="patternImage3" src={{this.dataValues.patternImage}} alt="Card image cap">
                    <div class="card-body">
                        <h5 class="card-title" id="patternName3">{{this.dataValues.patternName}}</h5>
                        <p class="card-text" id="patternAuthor3">{{this.dataValues.patternAuthor}}</p>
                        <a href={{this.dataValues.patternUrl}} id="patternLink3" class="btn btn-primary" target="_blank">Link</a>
                        <a data-id={{this.dataValues.patternId}} id="patternSave3" class="btn btn-primary unsave-btn">Unsave</a>
                    </div>
                </div>
            {{/each}}
        {{else}}
        <h1>No Saved Patterns</h1>
        {{/if}}
    </div>
</main>